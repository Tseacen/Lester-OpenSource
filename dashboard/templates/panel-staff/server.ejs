<%- include("../partials/header", { bot, user, path, staff, admin, support, billing, title: "Panel Staff - Serveur" }) %>
<% if (alert) { %>
    <div class="alert-error">
        <span class="closebtn" onclick="this.parentElement.style.display='none';">&times;</span> 
        <%= alert %>
      </div>
<% } %>

<form method="POST">

<div class="line1-panel-staff">
    <input name="server" class="input-server" type="text" placeholder="Saisissez l'ID d'un serveur">
    <button type="submit" class="confirm-server">Entrer</button>
</div>
</form>

<% if(guildStatus) { %>
    <div class="card-server-panelstaff">
        <img src="<%= guildIcon %>" class="guild-iconserverlist-panelstaff img-thumbnail">
        <div class="name-server-panelstaff"><%= guildName %></div>
        <div class="p-server-panelstaff"><strong class="color-grey">Propriétaire :</strong> <%= guildOwner %></div>
        <div class="p-server-panelstaff"><strong class="color-grey">Instance :</strong> <%= guildShard %></div>
        <div class="p-server-panelstaff"><strong class="color-grey">Membres :</strong> <%= guildCount %></div>
        <div class="p-server-panelstaff"><strong class="color-grey">ID :</strong> <%= guildId %></div>
        <div class="p-server-panelstaff"><strong class="color-grey">Premium :</strong> <%= guildPremium %></div>
        <a href="/dashboard/<%= guildId %>"><button class="button-server-list">Accéder au serveur</button></a>
    </div>
<% } %>


<%- include("../partials/footer") %>